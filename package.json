{
  "name": "moo-color",
  "version": "0.1.2",
  "description": "A simple color library.",
  "main": "dist/moo-color.js",
  "types": "types/moo-color.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack --hide-modules",
    "watch": "npm run build -- --watch"
  },
  "keywords": [
    "color"
  ],
  "author": "archcoster@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/moo-color.git"
  },
  "bugs": {
    "url": "https://github.com/archco/moo-color/issues"
  },
  "homepage": "https://github.com/archco/moo-color#readme",
  "dependencies": {
    "color-name": "^1.1.3"
  },
  "devDependencies": {
    "@types/color-name": "^1.1.0",
    "@types/jest": "^22.2.2",
    "jest": "^22.4.3",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts}",
      "!**/node_modules/**"
    ]
  }
}
