{
  "name": "moo-color",
  "version": "0.1.0",
  "description": "A simple color library.",
  "main": "dist/moo-color.js",
  "types": "types/moo-color.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "npx jest",
    "build": "npx webpack --hide-modules",
    "watch": "npm run build -- --watch"
  },
  "keywords": [
    "color"
  ],
  "author": "archcoster@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/moo-color.git"
  },
  "bugs": {
    "url": "https://github.com/archco/moo-color/issues"
  },
  "homepage": "https://github.com/archco/moo-color#readme",
  "dependencies": {
    "@types/color-name": "^1.1.0",
    "color-name": "^1.1.3"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "jest": "^22.1.4",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.0.2",
    "ts-loader": "^3.3.1",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-notifier": "^1.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
